    "eslint-plugin-simple-import-sort": "^12",
    "eslint-plugin-unused-imports": "^3",
    "jsdom": "^24",
    "postcss": "^8",
    "prettier": "^3",
    "prettier-plugin-tailwindcss": "^0",
    "sass": "^1",
    "stylelint": "^16",
    "stylelint-config-recess-order": "^5",
